@model WebApplication1.Models.Papiers.Devis
@{
    Layout = null;
    ViewBag.Title = "pdf";
}
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title</title>
    <link href="~/Content/bootstrap.css" rel="stylesheet" />
    @Scripts.Render("~/bundles/modernizr")
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/site")
    <link rel="shortcut icon" href="~/favicon.ico" />
</head>
<body>
    <div class="py-5">
        <div class="container">
            <div class="row">
                <div class="col-xs-6">
                    <p class="lead">Objet : @Html.DisplayFor(model => model.Objet)</p>
                </div>
                <div class="col-xs-6">
                    <p class="lead">Date : @Html.DisplayFor(model => model.Date)</p>
                </div>
                <div class="col-xs-6">
                    <p class="lead">Validité : @Html.DisplayFor(model => model.Valide)</p>
                </div>
                <div class="col-xs-6">
                    <p class="lead">Commentaire : @Html.DisplayFor(model => model.Commentaire)</p>
                </div>
                <div class="col-xs-6">
                    <p class="lead">Monnaie : @Html.DisplayFor(model => model.Monnaie)</p>
                </div>
            </div>
        </div>
    </div>
    <!-- Bootstrap Js CDN -->
    <script src="~/Scripts/bootstrap.min.js"></script>
    <script src="~/Scripts/bootstrap.js"></script>
</body>
</html>